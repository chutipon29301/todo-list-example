{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
      "no-unused-expression": true
    },
    "rules": {
        "object-literal-sort-keys": false,
        "quotemark": [true, "single"],
        "no-console": [true],
        "max-line-length": [true, {"limit": 140, "ignore-pattern": "^import | //"}],
        "ordered-imports": false,
        "interface-name": false
    },
    "rulesDirectory": []
}